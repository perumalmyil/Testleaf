package Week3.Classroom;

public class Java {
	public void reportStep(String msg, String status) {
		System.out.println("String for msg and status");
	}
	public void reportStep(String msg, String status, boolean snap) {
		System.out.println("msg and status and sanp");
		if(snap) {System.out.println("take a snap");}
		else {System.out.println("no need to take a snap");}
	}
	public void reportStep(String b, int a) {
		System.out.println("with String as first and int as next in argument");
	}
	public static void main(String[] args) {
		
	}

}
